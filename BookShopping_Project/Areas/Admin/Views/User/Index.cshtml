<div class="row">
    <div class="col-9 border-bottom">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-3">
        <a asp-area="Identity" class="btn btn-primary" asp-page="/Account/Register">
            <i class="fas fa-plus"></i>&nbsp;
            Create New User
        </a>
    </div>
</div>
<br />
<div class="border p-2 m-2">
    <table id="tblData" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>PhoneNumber</th>
                <th>Company</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
    <script src="~/js/User.js"></script>
}
